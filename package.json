    "@babel/preset-env": "^7.16.7",
    "babel-jest": "^27.4.6",
    "eslint": "^8.6.0",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.0.1",
    "jest-runner-groups": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.63.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
