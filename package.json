{
  "name": "leiskator",
  "version": "0.1.0",
  "description": "Leiskaa pukkaa",
  "repository": "https://github.com/mjvesa/leiskator",
  "icon": "public/images/unide_tiny.png",
  "publisher": "mjvesa",
  "main": "public/extension.js",
  "bin": "./cli.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "dev": "npm-run-all --parallel start watch",
    "start": "serve public",
    "test": "jest --group=unit",
    "test-all": "jest --group=unit --group=selenium",
    "test-coverage": "jest --group=unit --group=selenium --coverage",
    "test-selenium": "jest --group=selenium"
  },
  "author": "Matti Vesa",
  "categories": [
    "Other"
  ],
  "license": "Copyright (c) 2021 Matti Vesa, parts from Unide are MIT licenced",
  "engines": {
    "vscode": "^1.32.0"
  },
  "activationEvents": [
    "onCommand:extension.leiskator"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.leiskator",
        "title": "Leiskator"
      }
    ]
  },
  "dependencies": {
    "@vaadin/vaadin-core": "^21.0.9",
    "snabbdom": "^3.2.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@types/vscode": "^1.63.1",
    "babel-jest": "^27.4.6",
    "eslint": "^8.6.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.63.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "selenium-webdriver": "^4.1.0",
    "serve": "^13.0.2"
  }
}
